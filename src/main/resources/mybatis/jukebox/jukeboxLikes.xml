<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.jukebox.dao.JukeboxLikesDao">

  <insert id="insert" useGeneratedKeys="true" keyProperty="jlid" keyColumn="jlid">
    insert into jukebox_likes (mid, jid)
    values (#{mid}, #{jid})
  </insert>

  <select id="selectByJidAndMid" resultType="JukeboxLikes">
    select * from jukebox_likes
    where jid = #{jid} and mid = #{mid}
  </select>

  <delete id="delete">
    delete from jukebox_likes
    where jid = #{jid} and mid = #{mid}
  </delete>


</mapper>