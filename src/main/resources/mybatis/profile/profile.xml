<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.profile.dao.ProfileDao">
  <insert id="insert" parameterType="Profile">
    INSERT INTO PROFILES (NICKNAME, mid)
    VALUES (#{nickname}, #{mid})
  </insert>
  
  <!-- <select id="existsByNickname" parameterType="String" resultType="int">
      SELECT COUNT(*)
      FROM profiles
      WHERE nickname = #{nickname}
  </select> -->

  <!-- <select id="selectByNickname" parameterType="String" resultType="Profile">
    select (*)
    from profiles
    where nickname=#{nickname}
  </select> -->

  <select id="selectByPid" parameterType="Long" resultType="Profile">
    SELECT (*)
    FROM profiles
    WHERE pid = #{pid}
  </select>
  
  <select id="selectByMid" parameterType="Long" resultType="Profile">
    SELECT (*)
    FROM profiles
    WHERE mid = #{mid}
  </select>

  <!-- <update id="update" parameterType="Profile"> 
    update profiles
    set nickname=#{nickname}
    where pid=#{pid}
  </update> -->
</mapper>