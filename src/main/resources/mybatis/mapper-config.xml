  <typeAliases>
    <typeAlias type="com.example.demo.auth.dto.Member" alias="Member" />
    <!-- <typeAlias type="com.example.demo.dto.friend.Friend" alias="Friend" /> -->
    <typeAlias type="com.example.demo.guestboard.dto.GuestBoard" alias="GuestBoard" />
    <typeAlias type="com.example.demo.guestboard.dto.GuestBoardListRequest" alias="GuestBoardListRequest" />
    <typeAlias type="com.example.demo.guestboard.dto.GuestBoardUpdateRequest" alias="GuestBoardUpdateRequest" />

    <typeAlias type="com.example.demo.jukebox.dto.Jukebox" alias="Jukebox" />
  </typeAliases>
